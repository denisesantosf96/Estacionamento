@model X.PagedList.IPagedList<EstabelecimentoTipoVeiculo>
@using X.PagedList.Mvc.Core;
@using X.PagedList.Web.Common;

<table class="table">
        <thead class="thead-dark">
        <tr>
            <th>Id</th>
            <th>Tipo de Ve√≠culo</th>
            
            <th>Valor</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
            <td>@Model[i].Id</td>
            <td>@Model[i].IdTipoVeiculo</td>
            <td>@Model[i].Valor</td>
            <td><input type="button" name="EditarEstabelecimentoTipoVeiculo" value="Editar" class="btn btn-dark" onclick="EditarEstabelecimentoTipoVeiculo(@Model[i].Id)">
            <input type="button" name="ExcluirEstabelecimentoTipoVeiculo" value="Excluir" class="btn btn-danger" onclick="Excluir(@Model[i].Id)"></td>
            
            </tr>
            }
        
        </tbody>
    </table>

    @Html.PagedListPager(Model, pagina => Url.Action("Index", new { pagina }))